{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "angular-client"
  },
  "apps": [
    {
      "root": "./",
      "outDir": "dist",
      "assets": [
        "app/assets",
        "app/favicon.ico"
      ],
      "index": "app/index.html",
      "main": "app/main.ts",
      "test": "test.ts",
      "tsconfig": "app/tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "app/styles/main.scss"
      ],
      "scripts": [],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.dev.ts",
        "prod": "environments/environment.prod.ts"
       }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
